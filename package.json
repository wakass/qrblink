{
  "name": "qrblink",
  "version": "1.0.0",
  "description": "Qrblinker using webgl and qrcodegenerator",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^14.14.6",
    "babel-plugin-glslify": "^2.0.0",
    "glslify": "^7.1.1",
    "jquery": "^3.5.1",
    "three": "^0.122.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babelify": "^10.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "glslify"
          ]
        }
      ]
    ]
  },
  "babel": {
    "sourceType": "script"
  },
  "scripts": {
    "postinstall": "patch --forward node_modules/babel-plugin-glslify/glslify-babel.js < patches/glslbabel.patch"
  }
}
